/**index.wxss**/
.index-dox{
  height: 100%;
  position: relative;
  
}
.index-dox .dox-image{
  position: absolute;
  z-index: -1;
  top:0;
  left: 0;
  width: 100%;
  height: 100%;
}
.index-container{
  height: 100%;
  display: flex;
  flex-direction: column;
  justify-content: center;
  align-items: center;
}
.index-top{
  flex: 1;
}
.index-bottom{
  position: relative;
  display: flex;
  flex-direction: column;
  align-items: center;
  margin-bottom: 54rpx;
  width:686rpx;
  height:642rpx;
  background:rgba(84,57,191,1);
  border-radius:40rpx;
}
.index-bottom  .bottom-image{
  position: absolute;
  width: 46rpx;
  height: 46rpx;
  top: -12rpx;
  left: 23rpx;
}
.index-bottom .index-title{
  margin: 31rpx 0 46rpx 0;
  height: 70rpx;
  width: 237rpx;
}
.index-bottom .index-title image{
height: 100%;
width: 100%;
}
.index-bottom .index-text{
  flex:1;
  display: flex;
  margin-bottom: 28rpx;
  flex-direction: column;
  text-indent: 2em;
  font-size: 28rpx;
  color: #fff;
  width: 588rpx;
}
.index-bottom .index-button{
  display: flex;
  align-items: center;
  justify-content: center;
  width:476rpx;
  height:86rpx;
  font-size:32Rpx;
  font-family:PingFang SC;
  font-weight:bold;
  color:rgba(102,70,0,1);
  background:linear-gradient(0deg,rgba(229,180,0,1),rgba(255,221,1,1));
  border-radius:43rpx;
  margin-bottom: 36rpx;
}