/* pages/Order/Success.wxss */
.Success-container{
  display: flex;
  position: relative;
  z-index: 0;
  flex-direction: column;
  background: #F0F4F7;
  height: 100%;
  align-items: center;
}
.Success-container .Success-top{
  position: relative;
  display: flex;
  flex-direction: column;
  align-items: center;
}
.Success-container .Success-top>.img{
  position: absolute;
  z-index: -1;
  width: 750rpx;
  height: 410rpx;
}
.Success-container .Success-top .transaction{
  display: flex;
  margin-top: 73rpx;
  height: 42rpx;
  align-items: center;
}
.Success-container .Success-top .transaction .img{
  width: 28rpx;
  height: 28rpx;
  background: #fff;
  padding: 10rpx; 
  border-radius: 50%;
}
.Success-container .Success-top .transaction text{
  margin-left: 20rpx;
  font-size:42rpx;
  font-weight:400;
  color:rgba(255,255,255,1);
}
.Success-container .Success-top>text{
  font-weight:400;
  color:rgba(183,209,243,1);
  line-height:110rpx;
}
.Success-container .homepage{
  display: flex;
  width: 434rpx;
  justify-content: space-between;
}
.Success-container .homepage text{
  width:180rpx;
  height: 60rpx;
  border: 1rpx solid #fff;
  border-radius: 30rpx;
  font-weight:300;
  color:rgba(255,255,255,1);
  line-height:60rpx;
  text-align: center;
}

.Success-container .Success-bottom {
  position: relative;
  display: flex;
  flex-direction: column;
  margin-top: 49rpx;
  width: 640rpx;
  height: 100rpx;
  background: #fff;
  border-radius: 20rpx;
  padding: 40rpx 0 0 70rpx;
  overflow: hidden;
}
.Success-container .Success-bottom>text:nth-child(2){
  font-family:Microsoft YaHei;
  font-weight:400;
  color:#6993E0;
}
.Success-container .Success-bottom>text:nth-child(3){
  font-weight:300;
  color:rgba(102,102,102,1);
}
.Success-container .Success-bottom .img{
  position: absolute;
  width: 126rpx;
  height: 126rpx;
  right: 10rpx;
}